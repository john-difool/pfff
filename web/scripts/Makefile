TOP=../..
#############################################################################
# Configuration section
#############################################################################

##############################################################################
# Variables
##############################################################################

SRC =

LIBS=$(TOP)/commons/lib.cma \
 $(TOP)/globals/lib.cma

INCLUDEDIRS= $(TOP)/commons \
 $(TOP)/globals

SYSLIBS=bigarray.cma nums.cma str.cma unix.cma 

##############################################################################
# Generic variables
##############################################################################

-include $(TOP)/Makefile.common

##############################################################################
# Top rules
##############################################################################

all:: stat_web

stat_web: $(LIBS) $(EXTRALIBS) $(OBJS) main_stat_web.cmo
	$(OCAMLC) -custom -o $@ $(SYSLIBS) $^


##############################################################################
# Developer rules
##############################################################################

.PHONY:: stat

stat:
	./stat_web ../var/logs/access.log

